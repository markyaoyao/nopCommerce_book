            <div class="restuarent-tab nav nav-pills" id="pills-tab" role="tablist">
                <div class="home-category-slider owl-carousel mt-0">
                    @if (Model.SameCategories != null && Model.SameCategories.Count > 0)
                    {
                        foreach (var sameCategory in Model.SameCategories)
                        {
                            <div class="item">
                                <div class="nav-item" role="presentation">
                                    <button class="nav-link active" id="pills-tab-@sameCategory.Id" data-bs-toggle="pill" data-bs-target="#pills-tab-@sameCategory.Id" type="button" role="tab" aria-controls="pills-@sameCategory.Id" aria-selected="true" onclick="javascript: location.href='@Url.RouteUrl("Category", new { SeName = sameCategory.SeName })';">@sameCategory.Name</button>
                                </div>
                            </div>
                        }
                    }
